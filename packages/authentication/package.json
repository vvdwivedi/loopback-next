{
  "name": "@loopback/authentication",
  "version": "0.11.20",
  "description": "A LoopBack component for authentication support.",
  "engines": {
    "node": ">=8.9"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:resources": "lb-copy-resources",
    "clean": "lb-clean loopback-authentication*.tgz dist package api-docs",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/unit/**/*.js\" \"dist/__tests__/integration/**/*.js\" \"dist/__tests__/acceptance/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-authentication*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^0.13.1",
    "@loopback/core": "^0.11.16",
    "@loopback/metadata": "^0.9.14",
    "@loopback/openapi-v3": "^0.15.5",
    "@loopback/rest": "^0.25.5",
    "passport": "^0.4.0",
    "passport-strategy": "^1.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^0.7.6",
    "@loopback/openapi-spec-builder": "^0.9.5",
    "@loopback/testlab": "^0.15.1",
    "@types/node": "^10.11.2",
    "@types/passport": "^0.4.4",
    "@types/passport-http": "^0.3.6",
    "passport-http": "^0.3.0"
  },
  "keywords": [
    "LoopBack",
    "Authentication"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
